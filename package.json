{
  "name": "vite-app-demo",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=16.17"
  },
  "packageManager": "pnpm@7.14.0",
  "scripts": {
    "dev": "vite --mode development",
    "build:staging": "vue-tsc --noEmit && vite build --mode staging",
    "build": "vue-tsc --noEmit && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix src/",
    "lint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore src/",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint:style": "stylelint ./src/**/*.{css,vue} --fix",
    "lint:style:check": "stylelint ./src/**/*.{css,vue}",
    "prepare": "husky install",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@vueuse/head": "^0.9.8",
    "flowbite": "^1.5.3",
    "flowbite-vue": "^0.0.6",
    "pinia": "^2.0.23",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.14.2",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/node": "^18.11.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/test-utils": "^2.2.1",
    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "cssnano": "^5.1.14",
    "cssnano-preset-advanced": "^5.3.9",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "eslint-plugin-vue": "^9.6.0",
    "happy-dom": "^7.6.6",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.1",
    "vitest": "^0.24.3",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.9"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "@algolia/client-search": "^4.14.2"
  },
  "cssnano": {
    "preset": "advanced"
  }
}
