{
    "name": "vite-app-demo",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite --mode development",
        "build:staging": "vue-tsc --noEmit && vite build --mode staging",
        "build": "vite build --mode production --force",
        "preview": "vite preview --port=443",
        "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix src/",
        "lint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore src/",
        "format": "prettier . --write --ignore-path .gitignore",
        "format:check": "prettier . --check --ignore-path .gitignore",
        "lint:style": "stylelint ./src/**/*.{css,vue} --fix --ignore-path .gitignore",
        "lint:style:check": "stylelint ./src/**/*.{css,vue} --ignore-path .gitignore",
        "lint-staged": "lint-staged",
        "prepare": "cd .. && husky install",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@vueuse/core": "^9.12.0",
        "accessible-nprogress": "^2.1.2",
        "flowbite": "^1.6.3",
        "flowbite-vue": "^0.0.10",
        "pinia": "^2.0.30",
        "tailwindcss-animatecss": "^3.0.2",
        "vue": "^3.2.47",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@algolia/client-search": "^4.14.3",
        "@commitlint/cli": "^17.4.4",
        "@commitlint/config-conventional": "^17.4.4",
        "@iconify/json": "^2.2.23",
        "@intlify/unplugin-vue-i18n": "^0.8.2",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/line-clamp": "^0.4.2",
        "@types/node": "^18.14.0",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^5.52.0",
        "@unhead/vue": "^1.0.22",
        "@vitejs/plugin-basic-ssl": "^1.0.1",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vitest/coverage-c8": "^0.28.5",
        "@vue/compiler-dom": "^3.2.47",
        "@vue/compiler-sfc": "^3.2.47",
        "@vue/test-utils": "^2.2.10",
        "@vueuse/head": "^1.0.26",
        "@vueuse/schema-org": "^2.1.1",
        "autoprefixer": "^10.4.13",
        "c8": "^7.13.0",
        "cssnano": "^5.1.15",
        "cssnano-preset-advanced": "^5.3.10",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-define-config": "^1.15.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-tailwindcss": "^3.8.3",
        "eslint-plugin-vue": "^9.9.0",
        "eslint-plugin-vuejs-accessibility": "^2.1.0",
        "happy-dom": "^8.4.4",
        "husky": "^8.0.3",
        "less": "^4.1.3",
        "lint-staged": "^13.1.2",
        "postcss": "^8.4.21",
        "postcss-html": "^1.5.0",
        "prettier": "^2.8.4",
        "rollup": "^2.79.1",
        "stylelint": "^15.1.0",
        "stylelint-config-prettier": "^9.0.4",
        "stylelint-config-recommended": "10.0.1",
        "stylelint-config-recommended-vue": "^1.4.0",
        "stylelint-config-standard": "^30.0.1",
        "tailwindcss": "^3.2.7",
        "typescript": "^4.9.5",
        "unplugin-auto-import": "^0.14.3",
        "unplugin-icons": "^0.15.3",
        "unplugin-vue-components": "^0.24.0",
        "vite": "^4.1.2",
        "vite-plugin-cdn-import": "^0.3.5",
        "vite-plugin-compression2": "^0.8.2",
        "vite-plugin-minify": "^1.5.2",
        "vite-plugin-pwa": "^0.14.4",
        "vite-plugin-webfont-dl": "^3.6.0",
        "vitest": "^0.28.5",
        "vue-eslint-parser": "^9.1.0",
        "vue-tsc": "^1.1.3",
        "workbox-window": "^6.5.4"
    },
    "lint-staged": {
        "*.{js,ts,tsx,jsx,vue}": [
            "eslint --ignore-path .gitignore --fix",
            "prettier --ignore-path .gitignore --write"
        ],
        "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
            "prettier --ignore-path .gitignore --parser json --write"
        ],
        "package.json": [
            "prettier --ignore-path .gitignore --parser json --write"
        ],
        "*.md": ["prettier --ignore-path .gitignore --write"]
    },
    "peerDependencies": {
        "@algolia/client-search": "^4.14.2",
        "@unhead/vue": ">=1.0.10",
        "@vue/compiler-dom": "^3.0.1",
        "@vue/compiler-sfc": "^3.2.45",
        "postcss": "^8.0.0",
        "rollup": "^2.79.1",
        "stylelint-config-prettier": "^9.0.4"
    },
    "cssnano": {
        "preset": "advanced"
    }
}
