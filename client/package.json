{
    "name": "vite-app-demo",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite --mode development",
        "build:staging": "vue-tsc --noEmit && vite build --mode staging",
        "build": "vite build --mode production",
        "preview": "vite preview --port=443",
        "lint": "eslint --fix src/",
        "lint:check": "eslint --ext .js,.ts,.vue --ignore-pattern '.gitignore' --fix-dry-run src/",
        "format": "prettier . --write --ignore-path .gitignore",
        "format:check": "prettier . --check --ignore-path .gitignore",
        "lint:style": "stylelint ./src/**/*.{css,vue} --fix --ignore-path .gitignore",
        "lint:style:check": "stylelint ./src/**/*.{css,vue} --ignore-path .gitignore",
        "lint-staged": "lint-staged",
        "prepare": "cd .. && husky",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@types/markdown-it": "^14.1.1",
        "@vee-validate/i18n": "^4.13.1",
        "@vee-validate/rules": "^4.13.1",
        "@vueuse/components": "^10.11.0",
        "@vueuse/core": "^10.11.0",
        "@vueuse/integrations": "^10.11.0",
        "accessible-nprogress": "^2.1.2",
        "fast-xml-parser": "^4.4.0",
        "markdown-it-anchor": "^9.0.1",
        "markdown-it-prism": "^2.3.0",
        "pinia": "^2.1.7",
        "primeflex": "^3.3.1",
        "primeicons": "^7.0.0",
        "primevue": "^3.52.0",
        "vee-validate": "^4.13.1",
        "vue": "^3.4.30",
        "vue-i18n": "^9.13.1",
        "vue-router": "^4.4.0",
        "yup": "^1.4.0"
    },
    "devDependencies": {
        "@algolia/client-search": "^4.24.0",
        "@commitlint/cli": "^19.3.0",
        "@commitlint/config-conventional": "^19.2.2",
        "@csstools/css-tokenizer": "^2.3.1",
        "@eslint/eslintrc": "^3.1.0",
        "@eslint/js": "^9.5.0",
        "@iconify/json": "^2.2.223",
        "@intlify/unplugin-vue-i18n": "^4.0.0",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/typography": "^0.5.13",
        "@types/eslint__js": "^8.42.3",
        "@types/node": "^20.14.9",
        "@typescript-eslint/parser": "^7.14.1",
        "@unhead/schema-org-vue": "^0.6.0",
        "@unhead/vue": "^1.9.14",
        "@vitejs/plugin-basic-ssl": "^1.1.0",
        "@vitejs/plugin-vue": "^5.0.5",
        "@vitest/coverage-v8": "^1.6.0",
        "@vue/compiler-dom": "^3.4.30",
        "@vue/compiler-sfc": "^3.4.30",
        "@vue/test-utils": "^2.4.6",
        "autoprefixer": "^10.4.19",
        "browserslist": "^4.23.1",
        "cssnano": "^7.0.3",
        "cssnano-preset-advanced": "^7.0.3",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-tailwindcss": "^3.17.4",
        "eslint-plugin-vue": "^9.26.0",
        "eslint-plugin-vuejs-accessibility": "^2.3.1",
        "fp-ts": "^2.16.6",
        "globals": "^15.6.0",
        "happy-dom": "^14.12.3",
        "husky": "^9.0.11",
        "lightningcss": "^1.25.1",
        "lint-staged": "^15.2.7",
        "markdown-it": "^14.1.0",
        "postcss": "^8.4.38",
        "postcss-import": "^16.1.0",
        "prettier": "^3.3.2",
        "rollup": "^4.18.0",
        "stylelint": "^16.6.1",
        "stylelint-config-recommended": "14.0.1",
        "stylelint-config-recommended-vue": "^1.5.0",
        "stylelint-config-standard": "^36.0.1",
        "tailwindcss": "^3.4.4",
        "typescript": "^5.5.2",
        "typescript-eslint": "^7.14.1",
        "unplugin-auto-import": "^0.17.6",
        "unplugin-icons": "^0.19.0",
        "unplugin-vue-components": "^0.27.2",
        "unplugin-vue-markdown": "^0.26.2",
        "unplugin-vue-router": "^0.10.0",
        "vite": "^5.3.1",
        "vite-plugin-compression2": "^1.1.1",
        "vite-plugin-minify": "^1.5.2",
        "vite-plugin-pages": "^0.32.3",
        "vite-plugin-pages-sitemap": "^1.7.1",
        "vite-plugin-pwa": "^0.20.0",
        "vite-plugin-vue-meta-layouts": "^0.4.3",
        "vite-plugin-webfont-dl": "^3.9.4",
        "vitest": "^1.6.0",
        "vue-eslint-parser": "^9.4.3",
        "vue-tsc": "^2.0.22",
        "workbox-window": "^7.1.0"
    },
    "lint-staged": {
        "*.{js,ts,tsx,jsx,vue}": [
            "eslint --ignore-pattern '.gitignore' --fix",
            "prettier --ignore-path .gitignore --write"
        ],
        "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
            "prettier --ignore-path .gitignore --parser json --write"
        ],
        "package.json": [
            "prettier --ignore-path .gitignore --parser json --write"
        ],
        "*.md": ["prettier --ignore-path .gitignore --write"]
    },
    "cssnano": {
        "preset": "advanced"
    },
    "browserslist": [">= 0.25%"],
    "pnpm": {
        "overrides": {
            "deep-equal": "npm:@nolyfill/deep-equal@^1",
            "hasown": "npm:@nolyfill/hasown@^1",
            "isarray": "npm:@nolyfill/isarray@^1",
            "string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@^1",
            "is-core-module": "npm:@nolyfill/is-core-module@^1"
        }
    }
}
