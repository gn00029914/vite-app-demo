{
  "name": "vite-app-demo",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build:staging": "vue-tsc --noEmit && vite build --mode staging",
    "build": "vite build --mode production --force",
    "preview": "vite preview --port=443",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix src/",
    "lint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore src/",
    "format": "prettier . --write --ignore-path .gitignore",
    "format:check": "prettier . --check --ignore-path .gitignore",
    "lint:style": "stylelint ./src/**/*.{css,vue} --fix --ignore-path .gitignore",
    "lint:style:check": "stylelint ./src/**/*.{css,vue} --ignore-path .gitignore",
    "lint-staged": "lint-staged",
    "prepare": "cd .. && husky install",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "accessible-nprogress": "^2.1.2",
    "flowbite": "^1.6.3",
    "flowbite-vue": "^0.0.10",
    "pinia": "^2.0.30",
    "tailwindcss-animatecss": "^3.0.2",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.14.3",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@iconify/json": "^2.2.20",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@unhead/vue": "^1.0.21",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.4",
    "@vue/compiler-dom": "^3.2.47",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/test-utils": "^2.2.10",
    "@vueuse/head": "^1.0.25",
    "@vueuse/schema-org": "^2.1.1",
    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "cssnano": "^5.1.14",
    "cssnano-preset-advanced": "^5.3.9",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-define-config": "^1.15.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tailwindcss": "^3.8.3",
    "eslint-plugin-vue": "^9.9.0",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "happy-dom": "^8.2.6",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.1.1",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.4",
    "stylelint": "^15.0.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^30.0.1",
    "tailwindcss": "^3.2.6",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.14.2",
    "unplugin-icons": "^0.15.2",
    "unplugin-vue-components": "^0.23.0",
    "vite": "^4.1.1",
    "vite-plugin-compression2": "^0.7.0",
    "vite-plugin-minify": "^1.5.2",
    "vite-plugin-pwa": "^0.14.3",
    "vite-plugin-webfont-dl": "^3.6.0",
    "vitest": "^0.28.4",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.24",
    "workbox-window": "^6.5.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,vue}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --ignore-path .gitignore --parser json --write"
    ],
    "package.json": ["prettier --ignore-path .gitignore --parser json --write"],
    "*.md": ["prettier --ignore-path .gitignore --write"]
  },
  "peerDependencies": {
    "@algolia/client-search": "^4.14.2",
    "@unhead/vue": ">=1.0.10",
    "@vue/compiler-dom": "^3.0.1",
    "@vue/compiler-sfc": "^3.2.45",
    "postcss": "^8.0.0"
  },
  "cssnano": {
    "preset": "advanced"
  }
}
