{
    "name": "vite-app-demo",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite --mode development",
        "build:staging": "vue-tsc --noEmit && vite build --mode staging",
        "build": "vite build --mode production --force",
        "preview": "vite preview --port=443",
        "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix src/",
        "lint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore src/",
        "format": "prettier . --write --ignore-path .gitignore",
        "format:check": "prettier . --check --ignore-path .gitignore",
        "lint:style": "stylelint ./src/**/*.{css,vue} --fix --ignore-path .gitignore",
        "lint:style:check": "stylelint ./src/**/*.{css,vue} --ignore-path .gitignore",
        "lint-staged": "lint-staged",
        "prepare": "cd .. && husky install",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@types/markdown-it": "^13.0.0",
        "@vueuse/components": "^10.3.0",
        "@vueuse/core": "^10.3.0",
        "@vueuse/integrations": "^10.3.0",
        "accessible-nprogress": "^2.1.2",
        "flowbite": "^1.8.1",
        "flowbite-vue": "0.0.17-next.0",
        "markdown-it-anchor": "^8.6.7",
        "markdown-it-prism": "^2.3.0",
        "pinia": "^2.1.6",
        "tailwindcss-animatecss": "^3.0.4",
        "vue": "^3.3.4",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.2.4"
    },
    "devDependencies": {
        "@algolia/client-search": "^4.19.1",
        "@commitlint/cli": "^17.6.7",
        "@commitlint/config-conventional": "^17.6.7",
        "@csstools/css-tokenizer": "^2.2.0",
        "@iconify/json": "^2.2.99",
        "@intlify/unplugin-vue-i18n": "^0.12.2",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/typography": "^0.5.9",
        "@types/node": "^20.4.8",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@typescript-eslint/parser": "^6.3.0",
        "@unhead/vue": "^1.2.2",
        "@vitejs/plugin-basic-ssl": "^1.0.1",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vitest/coverage-c8": "^0.33.0",
        "@vue/compiler-dom": "^3.3.4",
        "@vue/compiler-sfc": "^3.3.4",
        "@vue/test-utils": "^2.4.1",
        "@vueuse/head": "^1.1.26",
        "@vueuse/schema-org": "^2.2.0",
        "autoprefixer": "^10.4.14",
        "c8": "^8.0.1",
        "cssnano": "^6.0.1",
        "cssnano-preset-advanced": "^6.0.1",
        "eslint": "^8.46.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-define-config": "^1.23.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-tailwindcss": "^3.13.0",
        "eslint-plugin-vue": "^9.16.1",
        "eslint-plugin-vuejs-accessibility": "^2.1.0",
        "fp-ts": "^2.16.1",
        "happy-dom": "^10.9.0",
        "husky": "^8.0.3",
        "less": "^4.2.0",
        "lint-staged": "^13.2.3",
        "markdown-it": "^13.0.1",
        "postcss": "^8.4.27",
        "postcss-html": "^1.5.0",
        "prettier": "^3.0.1",
        "rollup": "^3.27.2",
        "stylelint": "^15.10.2",
        "stylelint-config-recommended": "13.0.0",
        "stylelint-config-recommended-vue": "^1.5.0",
        "stylelint-config-standard": "^34.0.0",
        "tailwindcss": "^3.3.3",
        "typescript": "^5.1.6",
        "unplugin-auto-import": "^0.16.6",
        "unplugin-icons": "^0.16.5",
        "unplugin-vue-components": "^0.25.1",
        "unplugin-vue-router": "^0.6.4",
        "vite": "^4.4.9",
        "vite-plugin-compression2": "^0.10.3",
        "vite-plugin-minify": "^1.5.2",
        "vite-plugin-pages": "^0.31.0",
        "vite-plugin-pages-sitemap": "^1.6.1",
        "vite-plugin-pwa": "^0.16.4",
        "vite-plugin-vue-layouts": "^0.8.0",
        "vite-plugin-vue-markdown": "^0.23.7",
        "vite-plugin-warmup": "^0.1.0",
        "vite-plugin-webfont-dl": "^3.7.6",
        "vitest": "^0.34.1",
        "vue-eslint-parser": "^9.3.1",
        "vue-tsc": "^1.8.8",
        "workbox-window": "^7.0.0"
    },
    "lint-staged": {
        "*.{js,ts,tsx,jsx,vue}": [
            "eslint --ignore-path .gitignore --fix",
            "prettier --ignore-path .gitignore --write"
        ],
        "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
            "prettier --ignore-path .gitignore --parser json --write"
        ],
        "package.json": [
            "prettier --ignore-path .gitignore --parser json --write"
        ],
        "*.md": ["prettier --ignore-path .gitignore --write"]
    },
    "cssnano": {
        "preset": "advanced"
    }
}
