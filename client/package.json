{
  "name": "vite-app-demo",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build:staging": "vue-tsc --noEmit && vite build --mode staging",
    "build": "vite build --mode production",
    "preview": "vite preview --port=443",
    "lint": "eslint --flag unstable_ts_config --flag unstable_config_lookup_from_file --config eslint.config.mts --fix src/",
    "lint:check": "eslint --flag unstable_ts_config --flag unstable_config_lookup_from_file --config eslint.config.mts --ext .js,.ts,.vue --ignore-pattern '.gitignore' --fix-dry-run src/",
    "format": "prettier . --write --ignore-path .gitignore",
    "format:check": "prettier . --check --ignore-path .gitignore",
    "lint:style": "stylelint ./src/**/*.{css,vue} --fix --ignore-path .gitignore",
    "lint:style:check": "stylelint ./src/**/*.{css,vue} --ignore-path .gitignore",
    "lint-staged": "lint-staged",
    "prepare": "cd .. && husky",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@primevue/themes": "^4.2.3",
    "@types/markdown-it": "^14.1.2",
    "@vee-validate/i18n": "^4.14.7",
    "@vee-validate/rules": "^4.14.7",
    "@vueuse/components": "^11.3.0",
    "@vueuse/core": "^11.3.0",
    "@vueuse/integrations": "^11.3.0",
    "accessible-nprogress": "^2.1.2",
    "fast-xml-parser": "^4.5.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-prism": "^2.3.0",
    "pinia": "^2.2.6",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.3",
    "tailwindcss-primeui": "^0.3.4",
    "vee-validate": "^4.14.7",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.4.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@algolia/client-search": "^5.15.0",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@csstools/css-tokenizer": "^3.0.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@iconify/json": "^2.2.276",
    "@intlify/unplugin-vue-i18n": "^6.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.15",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.9.3",
    "@typescript-eslint/parser": "^8.15.0",
    "@unhead/schema-org-vue": "^0.6.0",
    "@unhead/vue": "^1.11.13",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vitest/coverage-v8": "^2.1.5",
    "@vue/compiler-dom": "^3.5.13",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "browserslist": "^4.24.2",
    "cssnano": "^7.0.6",
    "cssnano-preset-advanced": "^7.0.6",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-vue": "^9.31.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "fp-ts": "^2.16.9",
    "globals": "^15.12.0",
    "happy-dom": "^15.11.6",
    "husky": "^9.1.7",
    "jiti": "^2.4.0",
    "lightningcss": "^1.28.2",
    "lint-staged": "^15.2.10",
    "markdown-it": "^14.1.0",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "prettier": "^3.3.3",
    "rollup": "^4.27.4",
    "stylelint": "^16.10.0",
    "stylelint-config-recommended": "14.0.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0",
    "unplugin-auto-import": "^0.18.5",
    "unplugin-icons": "^0.20.1",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-markdown": "^0.26.2",
    "unplugin-vue-router": "^0.10.8",
    "vite": "^5.4.11",
    "vite-plugin-compression2": "^1.3.1",
    "vite-plugin-minify": "^2.0.1",
    "vite-plugin-pages": "^0.32.3",
    "vite-plugin-pages-sitemap": "^1.7.1",
    "vite-plugin-pwa": "^0.21.0",
    "vite-plugin-vue-meta-layouts": "^0.5.1",
    "vite-plugin-webfont-dl": "^3.10.1",
    "vitest": "^2.1.5",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.10",
    "workbox-window": "^7.3.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,vue}": [
      "eslint --flag unstable_ts_config --flag unstable_config_lookup_from_file --config eslint.config.mts --ignore-pattern '.gitignore' --fix",
      "prettier --ignore-path .gitignore --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --ignore-path .gitignore --parser json --write"
    ],
    "package.json": ["prettier --ignore-path .gitignore --parser json --write"],
    "*.md": ["prettier --ignore-path .gitignore --write"]
  },
  "cssnano": {
    "preset": "advanced"
  },
  "browserslist": [">= 0.25%"],
  "pnpm": {
    "overrides": {
      "deep-equal": "npm:@nolyfill/deep-equal@^1",
      "hasown": "npm:@nolyfill/hasown@^1",
      "isarray": "npm:@nolyfill/isarray@^1",
      "string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@^1",
      "is-core-module": "npm:@nolyfill/is-core-module@^1",
      "safe-buffer": "npm:@nolyfill/safe-buffer@^1"
    }
  }
}
