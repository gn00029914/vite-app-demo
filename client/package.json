{
    "name": "vite-app-demo",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite --mode development",
        "build:staging": "vue-tsc --noEmit && vite build --mode staging",
        "build": "vite build --mode production --force",
        "preview": "vite preview --port=443",
        "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix src/",
        "lint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore src/",
        "format": "prettier . --write --ignore-path .gitignore",
        "format:check": "prettier . --check --ignore-path .gitignore",
        "lint:style": "stylelint ./src/**/*.{css,vue} --fix --ignore-path .gitignore",
        "lint:style:check": "stylelint ./src/**/*.{css,vue} --ignore-path .gitignore",
        "lint-staged": "lint-staged",
        "prepare": "cd .. && husky install",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@types/markdown-it": "^13.0.7",
        "@vee-validate/i18n": "^4.12.4",
        "@vee-validate/rules": "^4.12.4",
        "@vueuse/components": "^10.7.2",
        "@vueuse/core": "^10.7.2",
        "@vueuse/integrations": "^10.7.2",
        "accessible-nprogress": "^2.1.2",
        "flowbite": "^2.2.1",
        "flowbite-vue": "0.0.17-next.4",
        "markdown-it-anchor": "^8.6.7",
        "markdown-it-prism": "^2.3.0",
        "pinia": "^2.1.7",
        "tailwindcss-animatecss": "^3.0.5",
        "vee-validate": "^4.12.4",
        "vue": "^3.4.15",
        "vue-i18n": "^9.9.0",
        "vue-router": "^4.2.5",
        "yup": "^1.3.3"
    },
    "devDependencies": {
        "@algolia/client-search": "^4.22.1",
        "@commitlint/cli": "^18.5.0",
        "@commitlint/config-conventional": "^18.5.0",
        "@csstools/css-tokenizer": "^2.2.3",
        "@iconify/json": "^2.2.172",
        "@intlify/unplugin-vue-i18n": "^2.0.0",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/typography": "^0.5.10",
        "@types/node": "^20.11.5",
        "@typescript-eslint/eslint-plugin": "^6.19.0",
        "@typescript-eslint/parser": "^6.19.0",
        "@unhead/schema-org-vue": "^0.6.0",
        "@unhead/vue": "^1.8.10",
        "@vitejs/plugin-basic-ssl": "^1.1.0",
        "@vitejs/plugin-vue": "^5.0.3",
        "@vitest/coverage-v8": "^1.2.1",
        "@vue/compiler-dom": "^3.4.15",
        "@vue/compiler-sfc": "^3.4.15",
        "@vue/test-utils": "^2.4.3",
        "autoprefixer": "^10.4.17",
        "cssnano": "^6.0.3",
        "cssnano-preset-advanced": "^6.0.3",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-define-config": "^2.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-tailwindcss": "^3.14.0",
        "eslint-plugin-vue": "^9.20.1",
        "eslint-plugin-vuejs-accessibility": "^2.2.1",
        "fp-ts": "^2.16.2",
        "happy-dom": "^13.2.1",
        "husky": "^8.0.3",
        "less": "^4.2.0",
        "lint-staged": "^15.2.0",
        "markdown-it": "^14.0.0",
        "postcss": "^8.4.33",
        "postcss-html": "^1.6.0",
        "prettier": "^3.2.4",
        "rollup": "^4.9.6",
        "stylelint": "^16.2.0",
        "stylelint-config-recommended": "14.0.0",
        "stylelint-config-recommended-vue": "^1.5.0",
        "stylelint-config-standard": "^36.0.0",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.3.3",
        "unplugin-auto-import": "^0.17.3",
        "unplugin-icons": "^0.18.2",
        "unplugin-vue-components": "^0.26.0",
        "unplugin-vue-markdown": "^0.25.2",
        "unplugin-vue-router": "^0.7.0",
        "vite": "^5.0.12",
        "vite-plugin-compression2": "^0.11.0",
        "vite-plugin-minify": "^1.5.2",
        "vite-plugin-pages": "^0.32.0",
        "vite-plugin-pages-sitemap": "^1.6.1",
        "vite-plugin-pwa": "^0.17.4",
        "vite-plugin-vue-meta-layouts": "^0.4.1",
        "vite-plugin-webfont-dl": "^3.9.1",
        "vitest": "^1.2.1",
        "vue-eslint-parser": "^9.4.1",
        "vue-tsc": "^1.8.27",
        "workbox-window": "^7.0.0"
    },
    "lint-staged": {
        "*.{js,ts,tsx,jsx,vue}": [
            "eslint --ignore-path .gitignore --fix",
            "prettier --ignore-path .gitignore --write"
        ],
        "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
            "prettier --ignore-path .gitignore --parser json --write"
        ],
        "package.json": [
            "prettier --ignore-path .gitignore --parser json --write"
        ],
        "*.md": ["prettier --ignore-path .gitignore --write"]
    },
    "cssnano": {
        "preset": "advanced"
    }
}
